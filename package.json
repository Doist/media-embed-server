    "keywords": [
        "oembed",
        "media",
        "proxy",
        "thumbnails"
    ],
    "author": "amix",
    "version": "2.2.2",
    "repository": "https://github.com/Doist/media-embed-server",
    "license": "MIT",
    "dependencies": {
        "async": ">=0.7.0",
        "commander": ">=0.6.0",
        "express": "^4.16.4",
        "imagesize": ">=1.0.0",
        "media-parser": ">=1.4.5",
        "memcached": "^2.2.2",
        "mime-types": "^2.1.21",
        "partial": "0.0.7",
        "request": ">=2.34.0",
        "webpage-info": ">=0.1.5"
    },
    "engines": {
        "node": ">=0.2.0"
    },
    "directories": {
        "lib": "./lib"
